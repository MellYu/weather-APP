{{#each this}}
<li class="timeWeather">
    <p class="timeWeather__time">{{localTime}}</p>
    <p class="timeWeather__temp">
        <img src="http://openweathermap.org/img/wn/{{weather}}@2x.png" alt="" class="timeWeather__temp-icon">
        <span class="timeWeather__temperature">{{temperature}} &#176;</span>
    </p>
    <ul class="timeWeather__data">
        <li class="timeWeather__pressure"><img src="./../images/preashure.png" alt="pressure" class="data__image">{{pressure}} mm</li>
        <li class="timeWeather__humidity"><img src="./../images/humidity.png" alt="humidity" class="data__image">{{humidity}}%</li>
        <li class="timeWeather__wind"><img src="./../images/wind.png" alt="wind" class="data__image">{{wind}} m/s</li>
    </ul>
</li>
{{/each}}